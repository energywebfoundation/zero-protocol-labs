datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model File {
  id        String   @id @default(uuid())
  fileName  String
  mimeType  String
  createdAt DateTime @default(now())
  content   Bytes

  @@index([createdAt])
}
