    "securitySchemes": {
      "api-key": {
        "type": "apiKey",
        "in": "header",
        "name": "X-API-KEY"
      }
    },
    "schemas": {
      "UploadFileDto": {
        "type": "object",
        "properties": {
          "file": {
            "type": "string",
            "format": "binary"
          }
        },
        "required": [
          "file"
        ]
      },
      "FileMetadataDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "5ff1cb39-da8b-4f0a-a17d-a5d00ea85a60"
          },
          "fileName": {
            "type": "string",
            "example": "certificate.pdf"
          },
          "mimeType": {
            "type": "string",
            "example": "application/pdf"
          },
          "createdAt": {
            "format": "date-time",
            "type": "string",
            "example": "2021-08-26T18:20:30.633Z"
          },
          "purchaseId": {
            "type": "string",
            "example": "3fc9c8da-4b6f-4976-be25-facfd13c5787"
          }
        },
        "required": [
          "id",
          "fileName",
          "mimeType",
          "createdAt"
        ]
      },
      "UpdateFileMetadataDto": {
        "type": "object",
        "properties": {
          "fileName": {
            "type": "string",
            "example": "certificate.pdf"
          },
          "mimeType": {
            "type": "string",
            "example": "application/pdf"
          },
          "purchaseId": {
            "type": "string",
            "example": "3fc9c8da-4b6f-4976-be25-facfd13c5787"
          }
        },
        "required": [
          "fileName",
          "mimeType"
        ]
      },
      "CreatePurchaseDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "04a7155d-ced1-4981-8660-48670a0735dd"
          },
          "certificateId": {
            "type": "string",
            "example": "973d48bb-15da-4eaf-8040-b6cb66e22023"
          },
          "buyerId": {
            "type": "string",
            "example": "29e25d61-103a-4710-b03d-ee12df765066"
          },
